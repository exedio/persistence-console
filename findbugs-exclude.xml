<FindBugsFilter>
	<Match>
		<!-- Could be refactored into a named static inner class -->
		<Bug pattern="SIC_INNER_SHOULD_BE_STATIC_ANON" />
		<Or>
			<Class name="com.exedio.cope.console.TransactionCop$1" />
			<Class name="com.exedio.cope.console.QueryCacheCop$Content$1" />
			<Class name="com.exedio.cope.console.HistoryTest$1" />
		</Or>
	</Match>
	<Match>
		<Or>
			<!-- A prepared statement is generated from a nonconstant String -->
			<Bug pattern="SQL_PREPARED_STATEMENT_GENERATED_FROM_NONCONSTANT_STRING" />
		</Or>
		<Or>
			<Class name="com.exedio.cope.console.HistoryPurge" />
			<Class name="com.exedio.cope.console.example.Revisions" />
		</Or>
	</Match>
	<Match>
		<Or>
			<!-- Class extends Servlet class and uses instance variables -->
			<Bug pattern="MTIA_SUSPECT_SERVLET_INSTANCE_FIELD" />
			<!-- Mutable servlet field -->
			<Bug pattern="MSF_MUTABLE_SERVLET_FIELD" />
			<!-- Non-transient non-serializable instance field in serializable class -->
			<Bug pattern="SE_BAD_FIELD" />
		</Or>
		<Or>
			<Class name="com.exedio.cope.console.ConsoleServlet" />
		</Or>
	</Match>
	<Match>
		<!-- Inefficient use of keySet iterator instead of entrySet iterator -->
		<Bug pattern="WMI_WRONG_MAP_ITERATOR" />
		<Or>
			<Class name="com.exedio.cope.console.EnvironmentCop" />
			<Class name="com.exedio.cope.console.Environment_Jspm" />
		</Or>
	</Match>
	<Match>
		<!-- may fail to clean up java.io.InputStream -->
		<Bug pattern="OBL_UNSATISFIED_OBLIGATION" />
		<Class name="com.exedio.cope.console.example.ExampleServlet" />
	</Match>
	<Match>
		<Or>
			<!-- Using notify() rather than notifyAll() -->
			<Bug pattern="NO_NOTIFY_NOT_NOTIFYALL" />
			<!-- Unconditional wait -->
			<Bug pattern="UW_UNCOND_WAIT" />
			<!-- Wait not in loop -->
			<Bug pattern="WA_NOT_IN_LOOP" />
		</Or>
		<Class name="com.exedio.cope.console.HistoryThread" />
	</Match>
</FindBugsFilter>

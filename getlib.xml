	<property name="getlib.home" value="${env.GETLIB_HOME}" />

	<import file="${getlib.home}/getlib-contrib.xml"/>


	<target name="getlib" depends="exedio-cope, exedio-cope-util, exedio-cops, exedio-jspm">
